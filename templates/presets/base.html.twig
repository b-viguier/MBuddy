{% extends "base.html.twig" %}

{% block title %}Presets{% endblock %}

{% block header %}
    <link rel="stylesheet" href="{{ asset('assets/css/MBuddy.css') }}" crossorigin="anonymous">
{% endblock %}

{% block body %}
    <div class="container">
        <h1>Presets</h1>

        {%  block presets_actions %}
        {% endblock %}

        <div class="row" id="presets-row">

            {% for preset in presets %}
                <div class="col-2 p-0">
                {% block preset %}
                {% endblock %}
                </div>
            {% endfor %}
        </div>
    </div>



{% endblock %}

