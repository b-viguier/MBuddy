{% extends "base.html.twig" %}

{% block title %}Motif - Masters{% endblock %}

{% block header %}
    <link rel="stylesheet" href="{{ asset('assets/css/MBuddy.css') }}" crossorigin="anonymous">
{% endblock %}

{% block body %}
    <div class="container">
        <h1>Motif - Masters</h1>

        {%  block motif_master_actions %}
        {% endblock %}

        <div class="row" id="motif-masters-row">

            {% for id,name in masters %}
                <div class="col-sm-3 col-lg-2 p-0">
                {% block master %}
                {% endblock %}
                </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}

